{
    "name": "2amigos/yii2-resource-manager-component",
    "description": "The resource manager component for the Yii framework",
    "keywords": [
        "2amigos",
        "yii",
        "yii2",
        "yii 2",
        "resource"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/2amigos/yii2-resource-manager-component",
    "authors": [
        {
            "name": "Alexander Kochetov",
            "email": "creocoder@gmail.com",
            "role": "Developer"
        },
        {
            "name": "2amigOS! Consulting Group",
            "email": "hola@2amigos.us",
            "homepage": "http://2amigos.us",
            "role": "Developer"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0",
        "aws/aws-sdk-php": "~2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "dosamigos\\resourcemanager\\": "src"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
